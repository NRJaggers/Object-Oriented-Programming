class Banana
{
private:
        string s;

        int y;

public:
bool theMemberFunc (string);

void setterForS(string input)     // setter for s
{
	s =  ;
}
string getterForS()               // getter for s
{
	cout << s << endl ;
}
};
void func(Banana) ;

int main()
{
	Banana co ;

	co.getterForS() ;
