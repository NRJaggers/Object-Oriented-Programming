#include <iostream>
using namespace std;

int main() {

	int * p ;
	p = new int ;
	*p = 10 ;

	cout << *p << endl;

	//*p--;
	(*p)--;

	*p = *p - 1
	cout << *p << endl;

	return 0;
}
